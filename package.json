  "bugs": {
    "url": "https://github.com/caleb531/truthy/issues"
  },
  "homepage": "https://github.com/caleb531/truthy#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "fastclick": "^1.0.6",
    "mithril": "^1.0.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "brunch": "^2.10.9",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "istanbul": "jacksonrayhamilton/istanbul#09f9660",
    "javascript-brunch": "^2.0.0",
    "karma": "^1.3.0",
